        </columns>
      </TableView>
      <Button fx:id="btnExportRodada" layoutX="71.0" layoutY="33.0" mnemonicParsing="false" onAction="#exportRodada" prefHeight="33.0" prefWidth="132.0" text="Exportar Rodada" />
      <Button fx:id="btnGerenciarRodada" layoutX="72.0" layoutY="96.0" mnemonicParsing="false" onAction="#nextScene" prefHeight="33.0" prefWidth="132.0" text="Gerenciar Rodada" />
      <Button fx:id="btnVoltar" layoutX="79.0" layoutY="335.0" mnemonicParsing="false" onAction="#previousScene" prefHeight="33.0" prefWidth="132.0" text="Voltar" />
   </children>
</AnchorPane>
