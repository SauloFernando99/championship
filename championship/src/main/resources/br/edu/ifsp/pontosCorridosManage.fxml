                <TableColumn fx:id="nomeTime" prefWidth="116.0" text="Time" />
                <TableColumn fx:id="pontosTime" prefWidth="65.0" text="Pontos" />
                <TableColumn fx:id="vitoriasTime" prefWidth="64.0" text="Vitorias" />
                <TableColumn fx:id="derrotasTime" prefWidth="67.0" text="Derrotas" />
